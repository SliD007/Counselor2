            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="500dp"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <RelativeLayout
                    android:id="@+id/rl13"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true">
                    <ImageView
                        android:id="@+id/imageLeftIm13"
                        android:layout_width="5dp"
                        android:src="@drawable/u125"
                        style="@style/image1"/>
                    <TextView
                        android:text="服务内容概述"
                        android:layout_toRightOf="@id/imageLeftIm13"
                        style="@style/text_worklog"/>

                    <ImageView
                        android:id="@+id/imageRightIm13"
                        style="@style/nextIm"/>
                </RelativeLayout>
                <EditText
                    android:id="@+id/editText13"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@id/rl13"
                    android:hint="请输入"
                    android:gravity="top"
                    android:textColorHint="#666"
                    android:textSize="16sp"
                    android:background="@null"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:nextFocusUp="@id/editText13"
                    android:nextFocusLeft="@id/editText13"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#ccc"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <RelativeLayout
                    android:id="@+id/rl16"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true">
                    <ImageView
                        android:id="@+id/imageLeftIm16"
                        android:layout_width="5dp"
                        android:src="@drawable/u125"
                        style="@style/image1"/>
                    <TextView
                        android:text="处理建议"
                        android:layout_toRightOf="@id/imageLeftIm16"
                        style="@style/text_worklog"/>

                    <ImageView
                        android:id="@+id/imageRightIm16"
                        style="@style/nextIm"/>
                </RelativeLayout>
                <EditText
                    android:id="@+id/editText16"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@id/rl16"
                    android:hint="    请输入"
                    android:gravity="top"
                    android:textColorHint="#666"
                    android:textSize="16sp"
                    android:background="@null"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:nextFocusUp="@id/editText16"
                    android:nextFocusLeft="@id/editText16"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#ccc"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
