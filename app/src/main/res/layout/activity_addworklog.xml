                    android:src="@drawable/u125"
                    style="@style/image1"/>
                <TextView
                    android:text="服务村社"
                    android:layout_toRightOf="@id/imageLeftIm01"
                    style="@style/text_worklog"/>
                <ImageView
                    android:id="@+id/imageRightIm01"
                    android:src="@drawable/u609"
                    style="@style/nextIm"/>
                <Spinner
                    android:id="@+id/spinner01"
                    android:layout_toLeftOf="@id/imageRightIm01"
                    style="@style/log_spinner"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#ccc"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <ImageView
                    android:id="@+id/imageLeftIm02"
                    android:layout_width="5dp"
                    android:src="@drawable/u125"
                    style="@style/image1"/>
                <TextView
                    android:text="服务对象姓名"
                    android:layout_toRightOf="@id/imageLeftIm02"
                    style="@style/text_worklog"/>
                <ImageView
                    android:id="@+id/imageRightIm02"
                    style="@style/nextIm"/>
                <EditText
                    android:id="@+id/editText02"
                    android:layout_toLeftOf="@id/imageRightIm02"
                    style="@style/log_spinner"
                    android:hint="请输入"
                    android:textColorHint="#ccc"
                    android:textSize="16sp"
                    android:nextFocusUp="@id/editText02"
                    android:nextFocusLeft="@id/editText02"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#ccc"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <ImageView
