                    android:text="身份证号"
                    android:layout_toRightOf="@id/imageLeftIm04"
                    style="@style/text_worklog"/>
                <ImageView
                    android:id="@+id/imageRightIm04"
                    style="@style/nextIm"/>
                <EditText
                    android:id="@+id/editText04"
                    android:layout_toLeftOf="@id/imageRightIm04"
                    android:background="@null"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:gravity="right"
                    android:hint="请输入"

                    android:textColorHint="#ccc"
                    android:textSize="14sp"
                    android:nextFocusUp="@id/editText04"
                    android:nextFocusLeft="@id/editText04"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#ccc"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <ImageView
                    android:id="@+id/imageLeftIm05"
                    android:layout_width="5dp"
                    style="@style/image1"/>
                <TextView
                    android:text="参与对象"
                    android:layout_toRightOf="@id/imageLeftIm05"
                    style="@style/text_worklog"/>
                <ImageView
                    android:id="@+id/imageRightIm05"
                    style="@style/nextIm"/>
                <EditText
                    android:id="@+id/editText05"
                    android:layout_toLeftOf="@id/imageRightIm05"
                    style="@style/log_spinner"
