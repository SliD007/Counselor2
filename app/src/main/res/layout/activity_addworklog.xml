                    android:textColorHint="#ccc"
                    android:textSize="16sp"
                    android:nextFocusUp="@id/editText05"
                    android:nextFocusLeft="@id/editText05"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#ccc"/>
